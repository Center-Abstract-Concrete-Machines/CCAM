---
import ProgramsLayout from '@layouts/ProgramsLayout.astro';
import { getCollection } from 'astro:content';

const filter = 'Event';
const events = await getCollection(
    'programs',
    ({ data }) => data.type === filter
);
---

<ProgramsLayout
    events={events}
    subtitle="Events"
    description="A custom description for events that is ONLY for events"
    filter={filter}
/>
